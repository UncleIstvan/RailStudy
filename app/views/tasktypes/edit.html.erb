<div class = row>
  <div class = "col-md-4">

    <div class="page-header">
      <h1>Edit type</h1>
    </div>

    <div class="panel panel-default">
      <div class="panel-heading">



        <%= form_for @tasktype do |f| %>
            <%= new_form f, :name%>
            <%= f.submit 'edit' %>
        <% end %>

        <%= link_to 'Go back to all tasks', tasktypes_path %>

      </div>
    </div>
  </div>
</div>